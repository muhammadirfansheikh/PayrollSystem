                                    AutoPostBack="true" OnSelectedIndexChanged="ddlCompanyNameSearch_SelectedIndexChanged">
                                </asp:DropDownList>
                            </div>
                            <div class="form-group col-lg-4">

                                <label for="exampleInputEmail2">Job Category</label>
