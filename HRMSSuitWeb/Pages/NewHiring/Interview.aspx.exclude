    </asp:UpdatePanel>


    <script type="text/javascript">

        function OpenDialog(a, b, c, Url) {
           
            sweetAlert(a, b, c),
            setTimeout(function () {

                if (Url != '#') {
                    window.location.replace(Url);
                }
            }, 2000);

        }

    </script>
</asp:Content>

