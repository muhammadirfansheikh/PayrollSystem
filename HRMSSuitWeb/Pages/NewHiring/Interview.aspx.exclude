                                    <label for="exampleInputEmail2">Department</label>
                                    <asp:Label runat="server" ID="lblCandidateDepartment" CssClass="exampleInputEmail2 label label-primary" Text=""></asp:Label>


                                </div>
                            </div>
                            <div class="row">
                                
                                <div class="form-group col-lg-4">
