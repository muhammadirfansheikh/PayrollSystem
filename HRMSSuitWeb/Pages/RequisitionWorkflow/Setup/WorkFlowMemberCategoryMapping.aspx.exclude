        function OpenDialog(a, b, c) {
            sweetAlert(a, b, c);
        }

        function OpenDialogError(a, b, c, Url) {

            sweetAlert(a, b, c),
            setTimeout(function () {

                if (Url != '#') {
                    window.location.replace(Url);
                }
            }, 2000);

        }

        function ApplyJob() {
            
            $('.ApplyJob').click();
